    "morgan": "^1.9.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-router-dom": "^4.3.1"
  },
  "scripts": {
    "start:react": "concurrently --kill-others \"nodemon server/index.js\" \"react-scripts start\"",
    "build-css": "node-sass-chokidar src/ -o src/",
